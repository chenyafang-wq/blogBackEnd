{
    "swagger": "2.0",
    "info": {
        "contact": {}
    },
    "paths": {
        "/login": {
            "post": {
                "description": "登录",
                "consumes": [
                    "application/json"
                ],
                "tags": [
                    "登录"
                ],
                "parameters": [
                    {
                        "type": "string",
                        "description": "账号",
                        "name": "account",
                        "in": "formData",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "密码",
                        "name": "password",
                        "in": "formData",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "获取信息成功"
                    },
                    "400": {
                        "description": "获取信息失败"
                    }
                }
            }
        },
        "/publish": {
            "post": {
                "description": "发布",
                "consumes": [
                    "application/json"
                ],
                "tags": [
                    "发布"
                ],
                "parameters": [
                    {
                        "type": "string",
                        "description": "文章内容",
                        "name": "artic",
                        "in": "formData",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "获取信息成功"
                    },
                    "400": {
                        "description": "获取信息失败"
                    }
                }
            }
        }
    }
}